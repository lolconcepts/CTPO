<h1>Listing of Parishioners</h1>
<table>
	<thead>
		<tr>
			<td>Print Name Tag</td>
			<td>Name</td>
			<td>Address</td>
			<td>City</td>
			<td>Email</td>
			<td>Phone</td>
		</tr>
	</thead>
<% @users.each do |user| %>
	<tr>
			<td><%= link_to 'Print NameTag', nametag_path(:uid => user.id) %> </td>
			<td><%= user.fullname%> </td>
			<td><%= user.address%> </td>
			<td><%= user.city%> </td>
			<td><%= user.email%> </td>
			<td><%= user.telephone %> </td>
		</tr>
<% end %>
</table>